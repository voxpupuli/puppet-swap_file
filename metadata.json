{
  "name": "puppet-swap_file",
  "version": "5.0.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Create swap files for Linux systems with Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-swap_file.git",
  "project_page": "https://github.com/voxpupuli/puppet-swap_file",
  "issues_url": "https://github.com/voxpupuli/puppet-swap_file/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "AlmaLinux",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Rocky",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Debian",
